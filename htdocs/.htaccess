# rewrites for the site..
Options +FollowSymlinks
RewriteEngine on
RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^pz/?(.*)$ /pz.php [L,QSA,B]
RewriteRule ^pz_login/?$ /pz.php [L,QSA,B]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^.*$ /web.php [L,QSA,B]
